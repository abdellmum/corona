{
  "name": "js",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
  
    "watch": "node-sass sass/main.scss dist/css/style.css -w",
    "server": "live-server --port=8181 --browser=chrome", 
    "start": "concurrently  \"npm run server\" \"npm run watch\"",

    "compiler": "node-sass sass/main.scss dist/css/style.css",
    "prefix": "postcss dist/css/style.css --use autoprefixer -o dist/css/style.prefix.css",
    "compress": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build": "npm-run-all compiler prefix compress",
   
    "fin-dev": "==============Branch dev==============",

    "deploying": "==============Branch Prod==============",
    "watch-deploy": "node-sass sass/main.scss dist/css/style.css -w",
    "start:deploy": "concurrently  \"npm run server:prod\" \"npm run watch-deploy\"",
    "deploying-app": "npm-run-all building-dev building-prod",
    "fin-deploying": "==============Branch dev==============",
    "Hebergement": "=====================================",
    "deploy": "gh-pages -d dist",
    "fin-Hebergement": "/=====================================/"

  },
  "author": "shdd",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "concurrently": "^5.1.0",
    "gh-pages": "^2.2.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "browserslist": [
    "defaults",
    "last 3 versions"
  ],
  "dependencies": {
    "package": "^1.0.1"
  },
  "description": ""
}
